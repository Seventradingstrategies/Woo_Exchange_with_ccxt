import pandas as pd
import numpy as np
import ccxt
import matplotlib.pyplot as plt
import APIKey
import time

ftx = ccxt.ftx()
#woo = ccxt.woo()
binance = ccxt.binance()

woo_market = "nothing"
ftx_market = ftx.load_markets()
binance = binance.load_markets()



#Connect with exchange using API Keys
woo = ccxt.woo({
    "enableRateLimit": True,
    "apiKey": APIKey.production_api_key,
    "secret": APIKey.production_api_secret_key
})

woo_orderbook = woo.fetch_order_book("SPOT_WOO_USDT")
first_ask_spot = woo_orderbook["asks"][0][0]
second_ask_perp = woo_orderbook["bids"][0][0]

print(first_ask_spot)
print(second_ask_perp)
